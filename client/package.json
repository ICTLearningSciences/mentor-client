{
  "name": "mentor-client",
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "format": "prettier --write '**/*.{js,jsx,json,ts,tsx}'",
    "develop": "npx gatsby develop",
    "start": "npm run develop",
    "build": "npx gatsby build --prefix-paths",
    "serve": "gatsby serve --prefix-paths",
    "clean": "gatsby clean",
    "license:fix": "license-check-and-add remove -f license-config.json && license-check-and-add add -f license-config.json",
    "test:license": "license-check-and-add check -f license-config.json",
    "test:audit": "npm audit --level=moderate",
    "test:format": "prettier --check '**/*.{js,jsx,json,ts,tsx}'",
    "test:lint": "eslint src --ext ts --ext tsx --ext js --ext jsx",
    "test:types": "tsc --noEmit"
  },
  "license": "MIT",
  "dependencies": {
    "@gradiant/xapi-dsl": "^1.10.0",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@reach/router": "^1.3.4",
    "gatsby": "^2.32.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-player": "^2.8.2",
    "react-redux": "^7.2.2",
    "react-scroll": "^1.8.1",
    "redux-cmi5": "github:beatthat/redux-cmi5#semver:^1.3.3",
    "redux-logger": "^3.0.6",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "devDependencies": {
    "@types/react-redux": "^7.1.16",
    "@types/react-scroll": "^1.8.2",
    "gatsby-plugin-env-variables": "^2.0.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-manifest": "^2.12.0",
    "gatsby-plugin-material-ui": "^2.1.10"
  }
}
